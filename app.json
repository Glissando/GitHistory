{
  "expo": {
    "name": "GitHistory",
    "slug": "GitHistory",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.warden.GitHistory",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.warden.GitHistory",
      "versionCode": 1
    },
    "description": "A mobile application for viewing Github statistics",
    "githubUrl": "https://github.com/Glissando/GitHistory"
  }
}
